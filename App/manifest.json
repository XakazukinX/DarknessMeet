{
  "description": "最も簡単な拡張機能",
  "manifest_version": 2,
  "name": "SimpleExtension",
  "version": "1.0",
  "icons": {
    "48": "SimpleExtension48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "test.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "グレースケールLOVE！"
  },
  "permissions": [
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ]
}